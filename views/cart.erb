<table class="table table-hover table-striped">
	<thead>
		<tr>
			<th> # </th>
			<th> Name </th>
			<th> Price </th>
			<th> Number </th>			
		</tr>
	</thead>
	<tbody>
		<% @orders_arr.each do |order| %>
			<tr>
				<td><%= order[0].id %></td>				
				<td><%= order[0].title %></td>
				<td><%= order[0].price %></td>
				<td><%= order[1]%></td>
			</tr>
		<% end %>	
	</tbody>
</table>